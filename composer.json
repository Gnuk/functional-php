{
    "name": "gnuk/functional",
    "description": "Functional tools",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gnuk\\Functional\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gnuk\\Test\\Functional\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Anthony Rey",
            "email": "arey@ippon.fr"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit --coverage-html coverage --coverage-text",
        "test:with-branches": "./vendor/bin/phpunit -d memory_limit=-1 --path-coverage --coverage-html coverage --coverage-text"
    }
}
